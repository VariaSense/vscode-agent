{
    "name": "variasense",
    "publisher": "VariaSense Inc.",
    "displayName": "VariaSense AI Agent",
    "description": "AI agent for Local and Online LLMs in VS Code",
    "version": "0.0.22",
    "repository": {
        "url": "https://wwww.github.com/VariaSense/vscode-agent"
    },
    "license": "MIT",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:variasense.chatView"
    ],
    "files": [
        "out",
        "media",
        "README.md",
        "package.json",
        "LICENSE"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "variasense.newChat",
                "title": "New Chat",
                "icon": "$(add)"
            },
            {
                "command": "variasense.showHistory",
                "title": "History",
                "icon": "$(history)"
            },
            {
                "command": "variasense.resetChat",
                "title": "Clear Chat",
                "icon": "$(clear-all)"
            },
            {
                "command": "variasense.openSettings",
                "title": "VariaSense AI Agent Settings",
                "icon": "$(gear)"
            }
        ],
        "menus": {
            "view/title": [
                {
                    "command": "variasense.newChat",
                    "when": "view == variasense.chatView",
                    "group": "navigation"
                },
                {
                    "command": "variasense.showHistory",
                    "when": "view == variasense.chatView",
                    "group": "navigation"
                },
                {
                    "command": "variasense.resetChat",
                    "when": "view == variasense.chatView",
                    "group": "navigation"
                },
                {
                    "command": "variasense.openSettings",
                    "when": "view == variasense.chatView",
                    "group": "navigation"
                }
            ]
        },
        "configuration": {
            "title": "VariaSense AI Agent",
            "properties": {
                "variasense.providerType": {
                    "type": "string",
                    "enum": [
                        "local",
                        "openai",
                        "anthropic"
                    ],
                    "default": "local",
                    "description": "The type of LLM provider to use."
                },
                "variasense.local.baseUrl": {
                    "type": "string",
                    "default": "http://127.0.0.1:1234/v1",
                    "description": "Base URL for the local LLM provider (e.g., LM Studio)."
                },
                "variasense.local.model": {
                    "type": "string",
                    "default": "",
                    "description": "Model identifier for the local provider. Leave empty to auto-detect."
                },
                "variasense.openai.apiKey": {
                    "type": "string",
                    "default": "",
                    "description": "API Key for OpenAI."
                }
            }
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "VariaSense",
                    "title": "VariaSense AI Agent",
                    "icon": "$(comment-discussion)"
                }
            ]
        },
        "views": {
            "VariaSense": [
                {
                    "type": "webview",
                    "id": "variasense.chatView",
                    "name": "VariaSense AI Agent"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.80.0",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "20.2.5",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "eslint": "^8.41.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^5.1.3",
        "@vscode/test-electron": "^2.3.2"
    }
}